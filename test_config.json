{
    // To avoid accidentally uploading your Discord API key to GitHub, do _not_ directly edit this example config file.
    // Instead, copy this file to somwhere private like ~/.et_discord_bot_config.json, and then test running the bot via
    // `make run_debug CONFIG_PATH="$HOME/.et_discord_bot_config.json"`

    "discord_api_auth_token": "<enter discord bot api auth token here>",
    "status_output_channel": <enter discord channel id here>,

    "bot_administrator": "<your name here>",
    "output_timezone": "US/Pacific",
    "db_url": "sqlite:///var/et_discord_bot_data/data.db",
    "game_name_display": "Wolfenstein: Enemy Territory",
    "server_filter": { // Example filtering to show only passwordless ET:L servers
        "game": "legacy",
        "needpass": "0"
    },
    "additional_servers": null
}
